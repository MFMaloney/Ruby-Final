<h1>Search for a member</h1>



<%= form_tag(users_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search Members" %>
<%= submit_tag "Search", name: nil %>
<% end %>


<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to(user.name, user) %>
    </li>
  <% end %>
</ul>